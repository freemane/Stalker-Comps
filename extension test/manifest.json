{
    "manifest_version": 2,

    "name": "Cookie Comps Extension",
    "description": "This extension is what we're using to learn about chrome extensions and interacting with cookies",
    "version": "1.0",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },

    "background": {
      "scripts": ["background.js"]
      },
    //
    //  "content_scripts": [
    //    {
    //      "matches": ["http://www.google.com/*"],
    //      "js": ["jquery-1.11.3.js","cookieDisplay.js"]
    //    }
    //  ],

    "permissions": [
    "activeTab",
        "tabs",
    "cookies",
    "history",
    "storage",
    "https://ajax.googleapis.com/",
    "https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js",
    "\u003Call_urls\u003E"
  ]
}
