{
  "manifest_version": 2,

  "name": "Getting started example",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
  },

  "background": {
  "scripts": ["cookieDisplay.js"]
  },
    
  "permissions": [
    "activeTab",
    "cookies",
    "*://*.google.com/*",
    "*://*.ebay.com/*",
    "https://ajax.googleapis.com/"
  ]
}