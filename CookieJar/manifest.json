{
    "manifest_version": 2,

    "name": "Cookie Comps Extension",
    "description": "This extension is what we're using to learn about chrome extensions and interacting with cookies",
    "version": "1.1",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },

    "background": {
      "scripts": ["js/background.js"]
      },
    
    "permissions": [
    "activeTab",
    "tabs",
    "cookies",
    "webRequest",
    "history",
    "storage",
    "\u003Call_urls\u003E"
  ]
}
